<%= render_ember_app :frontend do |head| %>
  <% head.append do %>
	<%= stylesheet_link_tag 'application', media: 'all'%>
	<%= javascript_include_tag 'application' %>
	<%= include_ember_stylesheet_tags :frontend %>
	<%= include_ember_script_tags :frontend %>
	<%= csrf_meta_tag %>
  <% end %>
<% end %>
